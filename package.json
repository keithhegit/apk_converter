{
  "name": "demo2apk",
  "version": "2.1.0",
  "private": true,
  "description": "Online HTML/React to APK build service",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeadWaveWave/demo2apk.git"
  },
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @demo2apk/backend dev",
    "worker": "pnpm --filter @demo2apk/backend worker",
    "test": "pnpm -r test",
    "test:core": "pnpm --filter @demo2apk/core test",
    "test:backend": "pnpm --filter @demo2apk/backend test",
    "test:e2e": "./scripts/test-e2e.sh",
    "frontend": "pnpm --filter @demo2apk/frontend dev",
    "clean": "rm -rf packages/*/dist packages/*/node_modules node_modules",
    "offlineify": "node packages/core/dist/utils/offlineify.js"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.0"
}